{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "supabase start",
      "type": "shell",
      "command": "supabase start"
    },
    {
      "label": "supabase db reset",
      "type": "shell",
      "command": "supabase db reset" // && cd bin && poetry run ./seed-all.sh"
    },
    {
      "label": "supabase generate types",
      "type": "shell",
      "command": "supabase gen types typescript --local --schema public > frontend/src/database.types.ts"
    },
    {
      "label": "redis start",
      "type": "shell",
      "command": "docker run -p 6379:6379 -it redis/redis-stack:latest"
    },
    {
      "label": "Start all",
      "dependsOn": ["supabase start", "redis start"],
      "problemMatcher": []
    }
  ]
}
